# Supabase configuration (public keys only)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# Runtime environment tag: dev | preview | prod
NEXT_PUBLIC_RUNTIME_ENV=dev

# Optional server-side overrides (defaults defined in src/config/defaults.ts)
COINGECKO_BASE_URL=https://api.coingecko.com/api/v3
RATE_LIMIT_REQUESTS_PER_MINUTE=60
CACHE_S_MAX_AGE_SECONDS=60
CACHE_STALE_WHILE_REVALIDATE_SECONDS=120
